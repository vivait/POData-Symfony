	"description": "POData Symfony2 MVC Controller bridge",
	"keywords": [ "rest", "odata", "symfony", "podata" ],
	"license": "MIT",
	"authors": [
		{
			"name": "Viva IT Limited",
			"email": "lewis@vivait.co.uk"
		}
	],
	"minimum-stability": "stable",

	"repositories": [
